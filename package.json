{
  "name": "simons-says-news",
  "version": "1.0.0",
  "description": "A full-stack React/Node.js news application",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon --config nodemon.json server.js",
    "client": "vite --port 5576",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --config nodemon.json server.js > server_output.log 2>&1",
    "dev:frontend": "vite",
    "dev:debug": "concurrently \"npm run dev:backend:debug\" \"npm run dev:frontend\"",
    "dev:backend:debug": "nodemon --config nodemon.json --inspect server.js",
    "restart": "node scripts/kill-ports.js && npm run dev",
    "build": "vite build",
    "heroku-postbuild": "npm run build",
    "kill-ports": "node scripts/kill-ports.js",
    "dev:legacy": "concurrently \"npm run server\" \"npm run client\"",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "setup:github": "node scripts/setup-github.js",
    "setup:heroku": "node scripts/setup-heroku.js",
    "setup:heroku-env": "node scripts/setup-heroku-env.js",
    "setup:auto-deploy": "node scripts/setup-auto-deploy.js",
    "setup:complete": "node scripts/complete-setup.js",
    "deploy": "git push heroku main",
    "clean": "node scripts/enhanced-cleanup.js",
    "fresh-start": "npm run clean && npm install && npm run dev",
    "health-check": "node scripts/health-check.js",
    "debug:heroku": "node scripts/heroku-debug.js",
    "setup:heroku-production": "node scripts/setup-heroku-production.js",
    "reset:eden": "node scripts/reset-project-eden.js",
    "reset:eden-all": "node scripts/reset-project-eden.js --all",
    "cleanup:duplicates": "node src/scripts/cleanup-duplicates.js",
    "migrate:unique-constraints": "node src/scripts/add-unique-constraints.js",
    "setup:prompts": "node src/scripts/setup-prompt-management.js",
    "setup:constraints": "node src/scripts/add-unique-constraints.js",
    "setup:logs": "node scripts/add-logs-table.js",
    "test:all": "node tests/run-all-tests.js",
    "test:comprehensive": "node tests/run-comprehensive-tests.js",
    "test:eden-validation": "node tests/run-all-tests.js",
    "test:stage3": "node test-stage3-core.js",
    "test:modules": "node tests/unit/test-news-aggregation.js && node tests/unit/test-image-sourcing.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.0",
    "firebase": "^11.8.1",
    "firebase-admin": "^13.4.0",
    "lucide-react": "^0.511.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-cron": "^4.0.7",
    "openai": "^4.103.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.8"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "vite",
    "full-stack"
  ],
  "author": "",
  "license": "MIT"
}
